{
    "name": "luca/qcsv",
    "description": "This tool represents a CSV manager tool.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luca-Andrei Codorean"
        }
    ],
    "require": {
      "ext-mbstring": "*",
        "nesbot/carbon": "^3.10",
        "symfony/dotenv": "^7.3",
        "symfony/flex": "^2.8",
        "symfony/framework-bundle": "^7.3",
        "symfony/console": "^7.3",
        "symfony/yaml": "^7.3",
        "symfony/runtime": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "symfony/monolog-bundle": "^3.0",
        "symfony/debug-bundle": "^7.3",
        "symfony/stopwatch": "^7.3",
        "symfony/web-profiler-bundle": "^7.3",
        "symfony/maker-bundle": "^1.64"
    }
}
